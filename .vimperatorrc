noremap j 5j
noremap k 5k
noremap J <C-d>
noremap K <C-u>
noremap <F11> gT
noremap <F12> gt
inoremap <F11> <Esc>gT
inoremap <F12> <Esc>gt
noremap <C-F11> :tabmove -1<CR>
noremap <C-F12> :tabmove +1<CR>
inoremap <C-F11> <Esc>:tabmove -1<CR>
inoremap <C-F12> <Esc>:tabmove +1<CR>

ignorekeys add digg\.com\/reader
ignorekeys add docs\.google\.com
ignorekeys add mail\.google\.com

set newtab=all
set complete=l
set focuscontent
set noscrollbars
set showstatuslinks=2
set editor="xterm -e vim"
set gui=none,tabs,navigation,addons
set hintchars=hjklasdfgyuipwertnmzxcvb

set nextpattern=\\bnext\\b,^>$,^(>>|»)$,^(>|»),(>|»)$,\\bmore\\b,(下|后|後)(一)?(页|頁|张|張|篇)
set previouspattern=\\bprev|previous\\b,^<$,^(<<|«)$,^(<|«),(<|«)$,(上|前)(一)?(页|頁|张|張|篇)
