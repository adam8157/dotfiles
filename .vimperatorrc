noremap a A
noremap j 5j
noremap k 5k
noremap J <C-d>
noremap K <C-u>
noremap <F11> gT
noremap <F12> gt
noremap s :stop<CR>
inoremap <F11> <Esc>gT
inoremap <F12> <Esc>gt
noremap <silent> <leader>a :set! extensions.autoproxy.proxyMode=auto<CR>
noremap <silent> <leader>g :set! extensions.autoproxy.proxyMode=global<CR>
noremap <silent> <leader>d :set! extensions.autoproxy.proxyMode=disabled<CR>

set complete=l
set guioptions=
set showstatuslinks=2
set wildoptions=auto,sort
set titlestring=Iceweasel
set editor=xterm\ -e\ vim

set nextpattern=\\s*(下一页|下一张|下一篇|下页|后页)\\s*,\\bnext\\b,^>$,^(>>|»)$,^(>|»),(>|»)$,\\bmore\\b
set previouspattern=\\s*(上一页|上一张|上一篇|上页|前页)\\s*,\\b(prev|previous)\\b,^<$,^(<<|«)$,^(<|«),(<|«)$

autocmd LocationChange .* js modes.passAllKeys = /.*(mail\.google\.com|www\.google\.com\/reader).*/.test(buffer.URL)
